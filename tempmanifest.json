{
    "name": "BWASP CRX",
    "short_name": "BWASP",
    "description": "Manual web vulnerability analyzer",
    "version" : "1.0",
    "default_locale": "en",
    "manifest_version": 3,
    "page action": {
    "default_title" : "BWASP CRX",
    "default_popup" : "html/popup.html"
    },
    "permissions": [
        "cookies",
        "storage",
        "debugger",
        "sessions",
        "tabs",
        "webRequest",
        "webNavigation",
        "http://*/*",
        "https://*/*"
      ]

}